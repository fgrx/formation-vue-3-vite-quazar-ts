{
  "name": "devwall",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview --port 5050",
    "typecheck": "vue-tsc --noEmit",
    "api": "json-server db.json --watch -m ./node_modules/json-server-auth -p 3001",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^1.0.1",
    "axios": "^0.26.0",
    "element-plus": "^2.0.4",
    "mitt": "^3.0.0",
    "pinia": "^2.0.11",
    "vue": "^3.2.31",
    "vue-router": "^4.0.13"
  },
  "devDependencies": {
    "@types/node": "^16.11.25",
    "@vitejs/plugin-vue": "^2.2.2",
    "@vue/tsconfig": "^0.1.3",
    "json-server": "^0.17.0",
    "json-server-auth": "^2.1.0",
    "sass": "^1.49.9",
    "typescript": "~4.5.5",
    "vite": "^2.8.4",
    "vitest": "^0.23.2",
    "vue-tsc": "^0.31.4"
  }
}
